{
    "name": "@meshx-org/ui",
    "description": "Component library used across MeshX Apps",
    "version": "0.0.3-unstable.1",
    "license": "apache-2.0",
    "author": "Roland Szarka-Kov√°cs <Roland.Szarka-Kovacs@meshx.co>",
    "homepage": "https://meshx.co",
    "main": "dist/index.js",
    "types": "src/index.ts",
    "files": [
        "src",
        "dist"
    ],
    "browserslist": [
        "defaults",
        "not IE 11"
    ],
    "publishConfig": {
        "access": "public",
        "registry": "https://npm.pkg.github.com",
        "main": "dist/index.js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/meshx-org/meshx-ui.git"
    },
    "scripts": {
        "prepare": "husky install",
        "prepublish": "npm run build",
        "format": "prettier --write \".\" ",
        "lint": "eslint 'src/**/*.{tsx,ts}'",
        "build": "tsc && yarn copy-css",
        "copy-css": "copyfiles -u 1 src/**/*.css dist/",
        "test": "echo \"Error: no test specified\" && exit 0",
        "storybook": "start-storybook -p 6006",
        "publish:unstable": "lerna publish prerelease --preid unstable --yes"
    },
    "dependencies": {
        "@storybook/react": "^6.5.10",
        "react-native-svg": "^13.3.0",
        "react-native-svg-web": "^1.0.9",
        "react-native-web": "^0.18.7",
        "react-slider": "^2.0.4"
    },
    "devDependencies": {
        "@babel/core": "^7.18.10",
        "@babel/preset-env": "^7.18.10",
        "@babel/preset-react": "^7.18.6",
        "@storybook/addon-docs": "^6.5.10",
        "@storybook/addon-actions": "^6.5.9 ",
        "@storybook/addon-essentials": "^6.5.10",
        "@storybook/addon-links": "^6.5.10",
        "@types/react-native": "^0.66.4",
        "@types/react-slider": "^1.3.1",
        "@typescript-eslint/eslint-plugin": "^5.38.0",
        "@typescript-eslint/parser": "^5.33.1",
        "babel-loader": "^8.2.5",
        "babel-plugin-react-native-web": "^0.18.7",
        "babel-preset-react-app": "^10.0.1",
        "copyfiles": "^2.4.1",
        "eslint": "^8.22.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-prettier": "^8.4.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-promise": "^6.0.0",
        "eslint-plugin-react": "^7.30.1",
        "eslint-plugin-react-native": "^4.0.0",
        "husky": "^8",
        "lerna": "^5.4.3",
        "metro-react-native-babel-preset": "^0.72.1",
        "prettier": "^2.7.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
