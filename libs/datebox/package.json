{
    "name": "@meshx-org/mxui-datebox",
    "description": "DateBox component for MXUI",
    "version": "0.7.46",
    "license": "Apache-2.0",
    "author": "Roland Szarka-Kov√°cs <Roland.Szarka-Kovacs@meshx.co>",
    "homepage": "https://meshx.co/produts/mxui",
    "sideEffects": false,
    "module": "lib/module/index.js",
    "main": "lib/commonjs/index.js",
    "types": "src/index.ts",
    "files": [
        "src",
        "lib"
    ],
    "react-native-builder-bob": {
        "source": "src",
        "output": "lib",
        "targets": [
            [
                "commonjs",
                {
                    "copyFlow": true
                }
            ],
            "module",
            "typescript"
        ]
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://npm.pkg.github.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/meshx-org/meshx-ui.git"
    },
    "scripts": {
        "build": "echo build"
    },
    "dependencies": {
        "clsx": "1.2.1",
        "@internationalized/date": "3.1.0",
        "@internationalized/number": "3.2.0",
        "@meshx-org/mxui-core": "0.7.46",
        "@meshx-org/mxui-primitives": "0.7.46"
    },
    "devDependencies": {
        "@types/styled-components": "^5.1.26",
        "react-native-builder-bob": "^0.20.3"
    },
    "peerDependencies": {
        "react-native": ">=0.69.0",
        "react": "^17.0.0 || ^18.0.0",
        "react-dom": "^17.0.0 || ^18.0.0",
        "styled-components": ">=5.0.0"
    },
    "nx": {
        "includedScripts": [
            "build"
        ],
        "targets": {
            "build": {
                "outputs": [
                    "{projectRoot}/lib/**"
                ]
            }
        }
    }
}
