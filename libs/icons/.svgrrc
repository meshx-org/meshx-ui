{
    "icon": 24,
    "typescript": true,
    "filenameCase": "kebab",
    "jsx": {
        "babelConfig": {
            "plugins": [
                [
                    "@svgr/babel-plugin-remove-jsx-attribute",
                    {
                        "elements": [
                            "svg"
                        ],
                        "attributes": [
                            "id",
                            "xmlns"
                        ]
                    }
                ],
                [
                    "@svgr/babel-plugin-replace-jsx-attribute-value",
                    {
                        "values": [
                            {
                                "value": "#000",
                                "newValue": "currentColor"
                            }
                        ]
                    }
                ]
            ]
        }
    }
}
