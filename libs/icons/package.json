{
    "name": "@meshx/mxui-icons",
    "description": "Icons components for MXUI",
    "version": "0.7.47",
    "license": "Apache-2.0",
    "author": "Roland Szarka-Kov√°cs <Roland.Szarka-Kovacs@meshx.co>",
    "homepage": "https://meshx.co/produts/mxui",
    "module": "lib/module/index.js",
    "main": "lib/commonjs/index.js",
    "types": "src/index.ts",
    "files": [
        "src",
        "lib"
    ],
    "react-native-builder-bob": {
        "source": "src",
        "output": "lib",
        "targets": [
            "commonjs",
            "module",
            "typescript"
        ]
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/meshx-org/meshx-ui.git"
    },
    "scripts": {
        "build": "pnpm exec svgr --out-dir src -- assets/icons && bob build"
    },
    "dependencies": {},
    "devDependencies": {
        "glob": "10.3.10",
        "@svgr/core": "8.1.0",
        "@svgr/cli": "8.1.0",
        "@svgr/plugin-svgo": "8.1.0",
        "@svgr/plugin-jsx": "8.1.0",
        "@svgr/plugin-prettier": "8.1.0"
    },
    "peerDependencies": {
        "react": "^17.0.0 || ^18.0.0",
        "react-dom": "^17.0.0 || ^18.0.0",
        "styled-components": ">=5.0.0"
    },
    "nx": {
        "includedScripts": [
            "build"
        ],
        "targets": {
            "build": {
                "inputs": [
                    "{projectRoot}/assets/icons/**"
                ],
                "outputs": [
                    "{projectRoot}/lib/**"
                ]
            }
        }
    }
}
