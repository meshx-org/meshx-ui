{
    "name": "@meshx-org/mxui-overlays",
    "description": "Overlay component for MXUI",
    "version": "0.7.45",
    "license": "Apache-2.0",
    "author": "Roland Szarka-Kov√°cs <Roland.Szarka-Kovacs@meshx.co>",
    "homepage": "https://meshx.co",
    "module": "lib/module/index.js",
    "main": "lib/commonjs/index.js",
    "types": "src/index.ts",
    "files": [
        "src",
        "lib"
    ],
    "react-native-builder-bob": {
        "source": "src",
        "output": "lib",
        "targets": [
            [
                "commonjs",
                {
                    "copyFlow": true
                }
            ],
            "module",
            "typescript"
        ]
    },
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/meshx-org/meshx-ui.git"
    },
    "scripts": {
        "build": "nx exec -- npx bob build"
    },
    "dependencies": {
        "classnames": "^2.3.2",
        "react-spring": "^9.7.3",
        "react-transition-group": "4.4.5",
        "@types/react-transition-group": "4.4.6",
        "@meshx-org/mxui-core": "0.7.45",
        "@meshx-org/mxui-primitives": "0.7.45",
        "@types/styled-components": "^5.1.26"
    },
    "devDependencies": {
        "react-native-builder-bob": "^0.20.3"
    },
    "peerDependencies": {
        "react": "^17.0.0 || ^18.0.0",
        "react-dom": "^17.0.0 || ^18.0.0",
        "styled-components": ">=5.0.0",
        "react-spring": "*"
    },
    "nx": {
        "includedScripts": [
            "build"
        ],
        "targets": {
            "build": {
                "outputs": [
                    "{projectRoot}/lib/**"
                ]
            }
        }
    }
}
