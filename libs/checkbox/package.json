{
    "name": "@meshx/mxui-checkbox",
    "description": "Checkbox component for MXUI",
    "version": "0.7.49",
    "license": "Apache-2.0",
    "author": "Roland Szarka-Kov√°cs <Roland.Szarka-Kovacs@meshx.co>",
    "homepage": "https://meshx.co/produts/mxui",
    "sideEffects": false,
    "module": "lib/module/index.js",
    "main": "lib/commonjs/index.js",
    "types": "src/index.ts",
    "files": [
        "src",
        "lib"
    ],
    "react-native-builder-bob": {
        "source": "src",
        "output": "lib",
        "targets": [
            [
                "commonjs",
                {
                    "copyFlow": true
                }
            ],
            "module",
            "typescript"
        ]
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/meshx-org/meshx-ui.git"
    },
    "scripts": {
        "build": "bob build"
    },
    "dependencies": {
        "react-stately": "3.29.1",
        "react-aria": "3.31.0",
        "@meshx/mxui-core": "0.7.49",
        "@meshx/mxui-icons": "0.7.49",
        "@meshx/mxui-text": "0.7.49"
    },
    "peerDependencies": {
        "react": "^17.0.0 || ^18.0.0",
        "react-dom": "^17.0.0 || ^18.0.0",
        "styled-components": ">=5.0.0"
    },
    "nx": {
        "includedScripts": [
            "build"
        ],
        "targets": {
            "build": {
                "inputs": [
                    "{projectRoot}/src/**"
                ],
                "outputs": [
                    "{projectRoot}/lib/**"
                ]
            }
        }
    }
}
