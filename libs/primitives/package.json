{
    "name": "@meshx/mxui-primitives",
    "description": "Button component for MXUI",
    "version": "0.7.46",
    "license": "Apache-2.0",
    "author": "Roland Szarka-Kov√°cs <Roland.Szarka-Kovacs@meshx.co>",
    "homepage": "https://meshx.co/produts/mxui",
    "module": "lib/module/index.js",
    "main": "lib/commonjs/index.js",
    "types": "src/index.ts",
    "files": [
        "src",
        "lib"
    ],
    "react-native-builder-bob": {
        "source": "src",
        "output": "lib",
        "targets": [
            [
                "commonjs",
                {
                    "copyFlow": true
                }
            ],
            "module",
            "typescript"
        ]
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://npm.pkg.github.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/meshx-org/meshx-ui.git"
    },
    "scripts": {
        "build": "bob build"
    },
    "dependencies": {
        "@meshx/mxui-core": "0.7.46",
        "styled-system": "^5.1.5",
        "@types/styled-system": "^5.1.15",
        "@types/styled-components": "^5.1.26"
    },
    "devDependencies": {
        "@types/react-native": "^0.70.8",
        "@types/styled-components-react-native": "^5.1.3",
        "react-native-builder-bob": "^0.23.2"
    },
    "peerDependencies": {
        "react": "^17 || ^18",
        "react-dom": "^17 || ^18",
        "styled-components": ">=5.0.0"
    },
    "nx": {
        "includedScripts": [
            "build"
        ],
        "targets": {
            "build": {
                "outputs": [
                    "{projectRoot}/lib/**"
                ]
            }
        }
    }
}
