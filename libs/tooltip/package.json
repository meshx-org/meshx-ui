{
    "name": "@meshx/mxui-tooltip",
    "description": "Tooltip component for MXUI",
    "version": "0.7.46",
    "license": "apache-2.0",
    "author": "Roland Szarka-Kov√°cs <Roland.Szarka-Kovacs@meshx.co>",
    "homepage": "https://meshx.co/produts/mxui",
    "module": "lib/module/index.js",
    "main": "lib/commonjs/index.js",
    "types": "src/index.ts",
    "files": [
        "src",
        "lib"
    ],
    "react-native-builder-bob": {
        "source": "src",
        "output": "lib",
        "targets": [
            [
                "commonjs",
                {
                    "copyFlow": true
                }
            ],
            "module",
            "typescript"
        ]
    },
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/meshx-org/meshx-ui.git"
    },
    "scripts": {
        "build": "pnpm exec bob build"
    },
    "dependencies": {
        "react-aria": "^3.31.1",
        "@react-stately/tooltip": "^3.4.6",
        "@react-aria/utils": "^3.23.0",
        "@react-aria/overlays": "^3.20.0",
        "@react-aria/tooltip": "^3.7.0",
        "@react-aria/focus": "^3.16.0",
        "@react-aria/interactions": "^3.20.1",
        "@spectrum-icons/ui": "3.6.3",
        "clsx": "2.1.0",
        "@meshx/mxui-overlays": "0.7.46"
    },
    "peerDependencies": {
        "react": "^17.0.0 || ^18.0.0",
        "react-dom": "^17.0.0 || ^18.0.0",
        "styled-components": ">=5.0.0"
    },
    "nx": {
        "includedScripts": [
            "build"
        ],
        "targets": {
            "build": {
                "outputs": [
                    "{projectRoot}/lib/**"
                ]
            }
        }
    }
}
