{
    "name": "@meshx/mxui-button",
    "description": "Button component for MXUI",
    "version": "0.7.49",
    "license": "apache-2.0",
    "author": "Roland Szarka-Kov√°cs <Roland.Szarka-Kovacs@meshx.co>",
    "homepage": "https://meshx.co/produts/mxui",
    "module": "lib/module/index.js",
    "main": "lib/commonjs/index.js",
    "types": "src/index.ts",
    "files": [
        "src",
        "lib"
    ],
    "react-native-builder-bob": {
        "source": "src",
        "output": "lib",
        "targets": [
            [
                "commonjs",
                {
                    "copyFlow": true
                }
            ],
            "module",
            "typescript"
        ]
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/meshx-org/meshx-ui.git"
    },
    "scripts": {
        "build": "bob build"
    },
    "dependencies": {
        "react-aria": "^3.31.1",
        "@react-aria/button": "^3.9.2",
        "@react-aria/focus": "^3.16.0",
        "@react-aria/utils": "^3.23.0",
        "@meshx/mxui-core": "0.7.49",
        "@meshx/mxui-text": "0.7.49",
        "@meshx/mxui-primitives": "0.7.49"
    },
    "devDependencies": {
        "@types/styled-components": "^5.1.26",
        "react-native-builder-bob": "^0.20.3"
    },
    "peerDependencies": {
        "react": "^17.0.0 || ^18.0.0",
        "react-dom": "^17.0.0 || ^18.0.0",
        "styled-components": ">=5.0.0"
    },
    "nx": {
        "includedScripts": [
            "build"
        ],
        "targets": {
            "build": {
                "outputs": [
                    "{projectRoot}/lib/**"
                ]
            }
        }
    }
}
